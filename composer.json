{
    "name": "ehystiv/sr-make",
    "description": "Simple Repositoy & Service generator for Laravel",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ehystiv\\SrMake\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stefano Bichicchi",
            "email": "ehystiv@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
