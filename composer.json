{
    "name": "ehystiv/sr-make",
    "description": "Simple Repositoy & Service generator for Laravel",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.32"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ehystiv\\SrMake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Ehystiv\\SrMake\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stefano Bichicchi",
            "email": "ehystiv@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "orchestra/testbench": "^9.5"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Ehystiv\\SrMake\\Providers\\PackageServiceProvider"
        ]
      }
    }
}
